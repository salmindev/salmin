@page
@model salmin.Pages.IndexModel1

<div class="container p-3">
	<div class="row p-4">
		<div class="col-6">
			<h2 class="text-primary">Category List</h2>
		</div>
		<div class="col-6">

		</div>
	</div>
	<br /><br />

	<table>
		<thead>
			<tr>
				<th>
					Category Name
				</th>
				<th>
					DisplayOrder
				</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var obj in Model.Categories)
			{
			<tr>
				<td width="50%">@obj.Name</td>
				<td width="50%">@obj.DispalyOrder</td>
			</tr>
			}
		</tbody>
	</table>
</div>
